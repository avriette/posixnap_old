# Process stack
alias pu='ps xawu | grep -i'
alias pa='ps xawu'
alias psme="ps xawu | grep $USER"

# Job control
alias j=jobs

# Directory listing
alias lt='ls -ltr | tail -f'
alias ll='ls -lag | less'

# Typos
alias bim='vim'
alias cim='vim'
alias cmod='chmod'
alias cown='chown'
alias cd..='cd ..'
alias cdd='cd' # my keyboard at work does this to me
alias zsg='zsh'
